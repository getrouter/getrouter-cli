{
  "name": "@getrouter/getrouter-cli",
  "version": "0.1.7",
  "type": "module",
  "description": "CLI for getrouter.dev",
  "bin": {
    "getrouter": "dist/bin.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsx src/bin.ts --help",
    "format": "biome check --write .",
    "lint": "biome check .",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.5",
  "dependencies": {
    "commander": "^14.0.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "@types/prompts": "^2.4.9",
    "tsdown": "^0.18.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.16"
  }
}
